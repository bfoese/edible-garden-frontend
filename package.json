{
  "name": "edible-garden-frontend",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/bfoese/edible-garden-frontend/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bfoese/edible-garden-frontend.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "ng": "ng",
    "start:dev:http": "ng serve --configuration=lang-en,http",
    "start:dev": "ng serve --configuration=lang-en --hmr",
    "start:dev:de": "ng serve --configuration=lang-de --hmr",
    "start:qa": "ng serve --configuration=qa --hmr",
    "start:prod": "ng serve --configuration=prod --hmr",
    "start:dev:nginx": "ng serve --configuration=https,dev-nginx",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && npm run style:lint",
    "e2e": "ng e2e",
    "api:gen": "rimraf projects/edible-garden-api/src/lib/rest-api/gen && cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 ng-openapi-gen --config openapi-edible-garden.config.json -o projects/edible-garden-api/src/lib/rest-api/gen",
    "style:lint": "stylelint \"projects/**/src/**/*.scss\" --syntax scss",
    "create:npmrc": "node create-npmrc.js",
    "seed:build:prod": "rimraf dist && ng build seed-sharing-app --prod",
    "seed:i18n:gen": "ng extract-i18n --project seed-sharing-app --output-path projects/seed-sharing-app/src/i18n/",
    "seed:i18n:merge": "xliffmerge --profile projects/seed-sharing-app/dev-config/xliffmerge.json en de",
    "seed:icons:gen": "cd projects/eg-ui-dynamic-icons && npm run icons:gen"
  },
  "engines": {
    "node": "14.15.x",
    "npm": "3.10.9"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@asymmetrik/ngx-leaflet": "^8.1.0",
    "@bfoese/eg-be-contract": "^0.0.25-snapshot",
    "@bfoese/eg-ui-components": "0.0.3",
    "@bfoese/eg-ui-core": "0.0.11",
    "@bfoese/eg-ui-fonts": "0.0.5",
    "@bfoese/eg-ui-icons": "0.0.8",
    "@bfoese/eg-ui-models": "^0.0.4",
    "@rxweb/types": "^1.0.6",
    "angular-split": "^5.0.0",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.7.1",
    "leaflet.tilelayer.colorfilter": "^1.2.5",
    "ng-samurai": "^2.0.5",
    "ng-zorro-antd": "^11.0.1",
    "object-fit-images": "^3.2.4",
    "rxjs": "~6.6.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@angular/localize": "^11.2.6",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/leaflet": "^1.5.21",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.3",
    "ng-openapi-gen": "^0.14.0",
    "ng-packagr": "^11.0.0",
    "ng-swagger-gen": "^2.3.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "sass-lint": "^1.13.1",
    "standard-version": "^9.0.0",
    "stylelint": "^13.10.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "svg-sprite": "^1.5.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.39.1",
    "typescript": "~4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "ci",
        "hidden": true
      },
      {
        "type": "build",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "perf",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ],
    "header": "Changelog",
    "commitUrlFormat": "{{host}}/{{owner}}/{{repository}}/commit/{{hash}}",
    "compareUrlFormat": "{{host}}/{{owner}}/{{repository}}/compare/{{previousTag}}...{{currentTag}}",
    "issueUrlFormat": "{{host}}/{{owner}}/{{repository}}/issues/{{id}}",
    "userUrlFormat": "{{host}}/{{user}}",
    "releaseCommitMessageFormat": "chore(release): {{currentTag}}",
    "issuePrefixes": [
      "#"
    ]
  }
}
